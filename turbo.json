{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["NEXT_PUBLIC_API_URI"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "env": ["NEXT_PUBLIC_API_URI"],
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"],
      "env": ["NEXT_PUBLIC_API_URI"]
    }
  },
  "ui": "tui"
}